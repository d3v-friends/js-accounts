{
  "name": "js-accounts",
  "version": "0.0.1",
  "description": "account manager",
  "scripts": {
    "bulid": "sh ./build.sh",
    "update": "pnpm update --recursive"
  },
  "dependencies": {
    "@js-mongo": "github:d3v-friends/js-mongo",
    "@js-pure": "github:d3v-friends/js-pure",
    "jsonwebtoken": "9.0.2",
    "mongodb": "6.3.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.5"
  },
  "main": "./cjs/index.cjs",
  "types": "./cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./mjs/index.mjs",
      "require": "./cjs/index.cjs",
      "types": "./ts/index.ts"
    },
    "./*": {
      "import": "./mjs/*",
      "require": "./cjs/*",
      "types": "./ts/*"
    }
  },
  "engines": {
    "node": "^20.10.0"
  }
}
